<section class="container-fluid py-5 mt-4 px-sm-4 px-md-5">
  <div class="bg-dark rounded-5 positio-relative overflow-hidden py-5 px-3 px-sm-4 px-xl-5 mt-2 mx-md-n3 mx-lg-auto">
    {# <div class="bg-banner position-absolute"></div> #}
    <div class="container position-relative pt-xl-4 pt-xxl-5 pb-sm-2">
      <div class="row pt-md-3 pt-lg-5">
        {% for item in bannerData %}
          <div class="col-lg-6 col-xxl-5 pb-lg-5 mb-xl-5">
            <h1 class="display-1 fw-bold text-center text-white text-lg-start mb-xxl-5">
              {{ item.title1 }}
              <span class="text-success">{{ item.title2 }}</span>
            </h1>
          </div>
          <div class="col-lg-5 col-xl-4 col-xxl-5 offset-lg-1 offset-xxl-2 d-flex flex-column pt-2 pt-md-4">
            <p class="fs-3 text-center text-light text-lg-start pb-2 pb-md-0 mb-4 mb-md-5">
              {{ item.sub_title }}
            </p>
            <div class="d-flex flex-column flex-sm-row align-items-center justify-content-center justify-content-lg-start pb-3 pb-sm-5">
              <a href="#"
                class="btn btn-lg btn-success fs-5 rounded-pill w-100 w-sm-auto me-sm-3 me-xl-4 mb-2 mb-sm-0">
                Let's partner
              </a>
              <a href="#"
                class="btn btn-lg btn-link text-white w-100 w-sm-auto px-2 text-capitalize text-decoration-none">
                view case studies &nbsp;<svg xmlns="http://www.w3.org/2000/svg"
                  width="16"
                  height="16"
                  fill="currentColor"
                  class="bi bi-arrow-right"
                  viewBox="0 0 16 16">
                  <path fill-rule="evenodd"
                    d="M1 8a.5.5 0 0 1 .5-.5h11.793l-3.147-3.146a.5.5 0 0 1 .708-.708l4 4a.5.5 0 0 1 0 .708l-4 4a.5.5 0 0 1-.708-.708L13.293 8.5H1.5A.5.5 0 0 1 1 8z" />
                </svg>
              </a>
            </div>
            <ul class="list-inline d-xl-flex text-center text-lg-start mt-auto mb-0 mb-sm-2">
              <li class="d-inline-flex align-items-center text-nowrap pt-1 me-4">
                <p class="text-light fs-5">
                  <svg xmlns="http://www.w3.org/2000/svg"
                    width="16"
                    height="16"
                    fill="currentColor"
                    class="bi bi-check2 text-success"
                    viewBox="0 0 16 16">
                    <path d="M13.854 3.646a.5.5 0 0 1 0 .708l-7 7a.5.5 0 0 1-.708 0l-3.5-3.5a.5.5 0 1 1 .708-.708L6.5 10.293l6.646-6.647a.5.5 0 0 1 .708 0z" />
                  </svg>&nbsp; Full spectrum of services
                </p>
              </li>
              <li class="d-inline-flex align-items-center text-nowrap pt-1">
                <p class="text-light fs-5">
                  <svg xmlns="http://www.w3.org/2000/svg"
                    width="16"
                    height="16"
                    fill="currentColor"
                    class="bi bi-check2 text-success"
                    viewBox="0 0 16 16">
                    <path d="M13.854 3.646a.5.5 0 0 1 0 .708l-7 7a.5.5 0 0 1-.708 0l-3.5-3.5a.5.5 0 1 1 .708-.708L6.5 10.293l6.646-6.647a.5.5 0 0 1 .708 0z" />
                  </svg>&nbsp; Flexible work terms
                </p>
              </li>
            </ul>
          </div>
        {% endfor %}
      </div>
    </div>
  </div>
</section>
